<?xml version="1.0"?>
<configuration>
   <location path="Profile.aspx">
    <system.web>
      <authorization>
        <deny users="?"/>
      </authorization>
    </system.web>
  </location>
  
<location path="UserProfile.aspx">
    <system.web>
      <authorization>
        <allow users="*"/>
      </authorization>
    </system.web>
  </location>

  <!--<location path="UserProfile.aspx">
    <system.web>
      <authorization>
        <deny users="?"/>
      </authorization>
    </system.web>
  </location>-->
  
<location path="AdminControls">
    <system.web>
      <authorization>
        <allow roles="Admin"/>
      </authorization>
    </system.web>
  </location>

</configuration>
